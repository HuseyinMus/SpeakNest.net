# Firebase yapılandırması - Gerçek değerlerle doldurun
NEXT_PUBLIC_FIREBASE_API_KEY=your_api_key_here
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id
NEXT_PUBLIC_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Uygulama URL'si - Geliştirme veya üretim ortamına göre güncelleyin
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Zoom API kimlik bilgileri - Zoom Marketplace'teki Server-to-Server OAuth uygulamanızdan alın
ZOOM_CLIENT_ID=your_zoom_client_id
ZOOM_CLIENT_SECRET=your_zoom_client_secret
ZOOM_ACCOUNT_ID=your_zoom_account_id

# TinyMCE API anahtarı (Zengin metin editörü için)
NEXT_PUBLIC_TINYMCE_API_KEY=your_tinymce_api_key

# API güvenlik anahtarı (scheduled meetings API endpoint'i için)
SCHEDULED_MEETINGS_API_KEY=your_scheduled_meetings_api_key
